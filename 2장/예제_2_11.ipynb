      "source": [
        "# 예제 2.11 ReLU 함수\n",
        "\n",
        "import numpy as np\n",
        "\n",
        "# ReLU 함수 정의\n",
        "def relu(net):\n",
        "    if net >= 0:\n",
        "        return net\n",
        "    else:\n",
        "        return 0\n",
        "\n",
        "x = np.array([1, -2])\n",
        "w = np.array([-1, 1])\n",
        "\n",
        "net = np.dot(x, w)   # 입력 가중합\n",
        "y = relu(net)        # 출력\n",
        "\n",
        "print(f\"net: {net} \\n y: {y}\")"
      ],
